{
  "name": "Proof of work",
  "description": "Test and score page elements having proof of work hash information",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
   {
     "matches": ["http://*/*", "https://*/*", "file:///*"],
     "css": ["content.css"],
     "js": ["jquery-3.3.1.min.js", "md5-proto.js", "content.js"]
   }
  ],
  "icons": 
  { 
	"16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "browser_action": {
    "default_icon" : "icon128.png",
    "default_title": "Proof of work content scoring client"
  },
  "manifest_version": 2
}
